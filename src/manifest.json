{
  "name": "Facebook Friend Renamer",
  "version": "0.1",
  "description": "Lets you rename facebook friends so you can identify them",
  "update_url": "http://github.com/tekkub/tender-checker/raw/master/updates.xml",
  "content_scripts": [
    {
      "matches": ["http://*.facebook.com/*"],
      "js": ["jquery-1.3.2.min.js", "content.js"],
      "run_at": "document_start"
    }
  ]
}